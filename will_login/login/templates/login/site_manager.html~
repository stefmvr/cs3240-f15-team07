<script>
    function colorChange(input, out, newColor, oldColor) {
        if(!out) {
            document.getElementById(input).style.backgroundColor = newColor;
        } else {
            document.getElementById(input).style.backgroundColor = oldColor;
        }
    }
</script>
<form action = "{% url 'login:site_manager_page' %}" method="post">
{% csrf_token %}
{%if makeList%}
        <select name = "userSelector" id="userSelector" style="float:left;">
            <option value="--default--">Users</option>
            {%for u in makeList%}
            <option value="{{u.user_name}}">{{u.user_name}}</option>
            {%endfor%}
        </select>
        <div style="float:left;height:21px; width:5px"></div>
    <input id = "make" type="submit" onmouseover="colorChange('make', false, '#001E77', '#5B84FF')" onmouseout="colorChange('make', true, '#001E77', '#5B84FF')" value="Make Site Manager" style="float:left;font-size: 100%; height:20px; width:155px ; border-radius: 4%; background-color: #5B84FF; color: white; font-family: Impact; border-width: 0"/>
{%endif%}

</form>
